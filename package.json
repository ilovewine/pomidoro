{
  "name": "pomidoro",
  "description": "Pomidoro pomodoro app for time tracking management",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "vite",
    "preview": "vite preview",
    "ts-check": "vue-tsc --noEmit",
    "build": "npm run ts-check && vite build",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore --fix src",
    "pretty": "prettier --write .",
    "ionic:serve": "npm start",
    "ionic:build": "npm run build",
    "generate": "ionic build && ionic cap copy && ionic cap sync"
  },
  "dependencies": {
    "@capacitor/android": "^5.0.1",
    "@capacitor/app": "^5.0.1",
    "@capacitor/core": "^5.0.1",
    "@capacitor/dialog": "^5.0.1",
    "@capacitor/haptics": "^5.0.6",
    "@capacitor/keyboard": "^5.0.1",
    "@capacitor/status-bar": "^5.0.1",
    "@ionic/storage": "^4.0.0",
    "@ionic/vue": "^7.0.5",
    "@ionic/vue-router": "^7.0.5",
    "cordova-sqlite-storage": "^6.1.0",
    "localforage-cordovasqlitedriver": "^1.8.0",
    "pinia": "2.0.36",
    "vue": "^3.2.47",
    "vue-router": "4.1.6",
    "vue3-colorpicker": "^2.2.2"
  },
  "devDependencies": {
    "@capacitor/cli": "^5.0.1",
    "@ionic/cli": "^7.1.1",
    "@types/jest": "^29.5.1",
    "@typescript-eslint/eslint-plugin": "5.59.5",
    "@typescript-eslint/parser": "5.59.5",
    "@vitejs/plugin-vue": "4.2.1",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/test-utils": "2.3.2",
    "@vue/vue3-jest": "^29.2.4",
    "cypress": "^12.11.0",
    "eslint": "8.40.0",
    "eslint-plugin-vue": "^9.11.1",
    "jest": "^29.5.0",
    "prettier": "2.8.8",
    "sass": "1.62.1",
    "ts-jest": "^29.1.0",
    "typescript": "5.0.4",
    "vite": "4.3.5",
    "vue-tsc": "^1.8.22"
  },
  "overrides": {
    "ionicons": "^6.0.0"
  }
}
